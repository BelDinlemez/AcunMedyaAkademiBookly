@model List<BooklyProject.Entities.PhotoGallery>
   
    
<section id="instagram">
    <div class="container">
        <div class="text-center mb-4">
            <h3>Fotoğraf Galerisi</h3>
        </div>
       

        <div class="row">
            @foreach (var item in Model)
            {
                <div class="col-md-2">

                    <figure class="instagram-item position-relative rounded-3">

                        <a href="https://www.instagram.com/bel1n4y/" class="image-link position-relative">

                            <div class="icon-overlay position-absolute d-flex justify-content-center">
                                <svg class="instagram">
                                    <use xlink:href="#instagram"></use>
                                </svg>
                            </div>
                            <img src="@item.ImageUrl" alt="instagram" style="width:200px;height:200px" class="img-fluid rounded-3 insta-image">
                        </a>
                       
                    </figure>

                </div>
            }
            </div>
        
    </div>
</section>
